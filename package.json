{
  "name": "Sans",
  "main": "lib/electron/index.js",
  "version": "0.0.0",
  "engines": {
    "node": ">=10.7.0"
  },
  "scripts": {
    "dev": "NODE_ENV=dev electron-forge start",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "forge-publish": "env-cmd .env electron-forge publish",
    "prettier": "prettier --write 'lib/**/*.js'",
    "lint": "eslint lib"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-emotion": "^9.2.6",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-compilers": "^5.9.0",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.5",
    "electron-rebuild": "^1.8.2",
    "eslint": "^5.3.0",
    "eslint-plugin-node": "^7.0.1",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "babel-polyfill": "^6.26.0",
    "bash-preexec": "git+ssh://git@github.com/rcaloras/bash-preexec#0.3.7",
    "electron-compile": "^6.4.3",
    "electron-is-dev": "^0.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^2.0.0",
    "emotion": "^9.2.6",
    "env-cmd": "^8.0.2",
    "escape-string-regexp": "^1.0.5",
    "eventemitter3": "^3.1.0",
    "fs-extra": "^7.0.0",
    "lodash": "^4.17.10",
    "mousetrap": "^1.6.2",
    "nanoid": "^1.2.3",
    "node-pty": "^0.7.6",
    "os-locale": "^2.1.0",
    "react": "^16.5.1",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "xterm": "^3.7.0"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "ignore": [
          "external"
        ]
      },
      "electronWinstallerConfig": {
        "name": "sans"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "gonstr",
        "name": "sans"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "sans"
      }
    }
  }
}
